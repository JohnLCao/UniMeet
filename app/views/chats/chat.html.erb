<div class="cntr chat-page">
	<div 
		id="tlkio" 
		data-channel="ThunderTurtle" 
		data-theme="theme--day" 
		data-nickname=<%= current_user.name %> 
		style="width:100%;height:400px;">		
	</div>
	<script async src="http://tlk.io/embed.js" type="text/javascript"></script>
	<hr>
	<%= link_to "Back", all_chats_path(current_user.id), {style: 'font-size: 17px;'}%> |
	<%= link_to "Logout", logout_path, {style: 'font-size: 17px;', method: :delete} %>
</div>