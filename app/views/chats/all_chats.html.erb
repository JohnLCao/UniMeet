<h1>Listing User Matches</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Bio</th>
      <th>Bool drink</th>
      <th>Party</th>
      <th>Study</th>
      <th>Commute</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
        <td><%= user.bio %></td>
        <td><%= user.bool_drink %></td>
        <td><%= user.party %></td>
        <td><%= user.study %></td>
        <td><%= user.commute %></td>
        <td><%= link_to "Start Chat!", one_chat_path%></td>
      </tr>
    <% end %>
  </tbody>
</table>